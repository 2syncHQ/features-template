{
  "name": "features-template",
  "author": "Simo Elalj <simo@2sync.com>",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "generate-slugs": "node --experimental-modules --experimental-specifier-resolution=node -r dotenv/config generate-slugs.mjs",
    "dev": "npm run generate-slugs && next dev",
    "build": "npm run generate-slugs && next build",
    "start": "npm run generate-slugs && next start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "dotenv": "16.0.3",
    "got": "12.1.0",
    "lqip-modern": "1.2.0",
    "next": "14.1.0",
    "notion-client": "6.16.0",
    "notion-utils": "6.16.0",
    "p-map": "5.3.0",
    "p-memoize": "6.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-notion-x": "6.16.0",
    "react-tweet-embed": "2.0.0",
    "react-use": "17.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.1.0",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "cross-env": "7.0.3",
    "typescript": "4.8.4"
  }
}